<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="1" verbose="1" name="WebFormsCore">
	<test name="AbcdTest" enabled="true" preserve-order="true">
		<groups>
			<run>
				<exclude name="droolsEngineRules" />
				<exclude name="gson" />
				<exclude name="pluginsTest" />
				<include name="performanceTest" />
			</run>
		</groups>
		<classes>
			<class name="com.biit.drools.test.DroolsEngineRulesTest" />
			<class name="com.biit.drools.test.GlobalVariablesSerializerTest" />
			<class name="com.biit.drools.test.PluginsTest" />
			<class name="com.biit.drools.test.PerformanceTest" />
		</classes>
	</test>
</suite>